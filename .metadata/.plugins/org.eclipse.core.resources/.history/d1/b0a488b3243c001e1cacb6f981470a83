	PR_APPROVED("PR_APPROVED"),
	PR_AUTH("PR_AUTH"),
	PR_AUTH_PROG("PR_AUTH_PROG"),
	PR_CONFIRMED("PR_CONFIRMED"),
	PR_ELIMINATED("PR_ELIMINATED"),
	PR_ELIMINATED_AUT("PR_ELIMINATED-AUT"),
	PR_REJECTED("PR_REJECTED"),
	PR_REWORK("PR_REWORK"),
	PR_REWORK_AUTH("PR_REWORK-AUTH"),
	RM_APPROVED("RM_APPROVED"),
	RM_AUTH("RM_AUTH"),
	RM_REJECTED("RM_REJECTED"),
	RM_RENEW_LETTER("RM_RENEW_LETTER"),
	RM_RENEW_VIPD("RM_RENEW_VIPD"),
	RM_RESUBMIT_AUTH("RM_RESUBMIT_AUTH"),
	RM_REWORK("RM_REWORK"),
	RM_REWORK_AUTH("RM_REWORK-AUTH"),
	RM_DEACTIVATE_AUTH("RM_DEACTIVATE_AUTH"),
	RM_DEACTIVATED("RM_DEACTIVATED"),
	RM_ACTIVATE_AUTH("RM_ACTIVATE_AUTH"),
