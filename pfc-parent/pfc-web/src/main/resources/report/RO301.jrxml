			<property name="com.jaspersoft.studio.field.label" value="RAW_MATL_NAME"/>
		</field>
		<field name="FINAL_STATUS" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="FINAL_STATUS"/>
		</field>
